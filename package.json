{
  "name": "font-awesome-openui5",
  "version": "2.1.0",
  "description": "Font Awesome icons for OpenUI5.",
  "main": "./common/font-awesome-openui5.common.js",
  "module": "./common/font-awesome-openui5.js",
  "typings": "./common/font-awesome-openui5.common.d.ts",
  "files": [
    "autoload.js",
    "common",
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "concurrently 'npm run build:node' 'npm run build:browser'",
    "build:node": "tsc -p .",
    "build:browser": "webpack",
    "clean": "rimraf ./common/ ./dist/",
    "lint": "npm run lint:ts && npm run lint:js",
    "lint:js": "eslint .",
    "lint:ts": "tslint -p ./tsconfig.json --type-check",
    "pretest": "concurrently 'npm run build' 'gulp count:test'",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "karma start",
    "test:node": "mocha -r ts-node/register ./test/node.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/font-awesome-openui5.git"
  },
  "keywords": [
    "font-awesome",
    "font awesome",
    "openui5"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/font-awesome-openui5/issues"
  },
  "homepage": "https://github.com/gluons/font-awesome-openui5",
  "dependencies": {
    "font-awesome-icon-chars": "^1.2.2",
    "lodash.defer": "^4.1.0",
    "lodash.isnil": "^4.0.0"
  },
  "devDependencies": {
    "@types/chai": "^3.5.0",
    "@types/gulp": "^4.0.2",
    "@types/mocha": "^2.2.40",
    "@types/node": "6",
    "@types/openui5": "^1.40.1",
    "@types/vinyl-source-stream": "^0.0.29",
    "@types/webpack": "^2.2.14",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.4.0",
    "chai": "latest",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-gluons": "^2.0.1",
    "gulp": "^3.9.1",
    "karma": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-json-preprocessor": "^0.3.3",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-webpack": "^2.0.3",
    "mocha": "latest",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.2",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^2.3.3"
  }
}
