{
  "name": "font-awesome-openui5",
  "version": "2.0.0",
  "description": "Font Awesome icons for OpenUI5",
  "main": "./main/font-awesome-openui5.js",
  "scripts": {
    "build:unmin": "webpack",
    "build:min": "webpack -p --output-filename font-awesome-openui5.min.js",
    "build": "npm run clean && npm run build:unmin && npm run build:min && npm run tsc",
    "clean": "rimraf ./dist ./main",
    "tsc": "tsc -p . --sourceMap false",
    "pretest": "gulp count:test",
    "test": "karma start ./karma.sync.conf.js && karma start ./karma.async.conf.js",
    "test:local": "karma start ./karma.sync.conf.js --browsers FirefoxDeveloper && karma start ./karma.async.conf.js --browsers FirefoxDeveloper"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/font-awesome-openui5.git"
  },
  "keywords": [
    "font-awesome",
    "font awesome",
    "openui5"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/font-awesome-openui5/issues"
  },
  "homepage": "https://gluons.github.io/font-awesome-openui5",
  "dependencies": {
    "font-awesome-icon-chars": "^1.2.2",
    "lodash.defer": "^4.1.0",
    "nvl": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "6",
    "@types/openui5": "^1.40.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "eslint": "^3.14.0",
    "eslint-config-gluons": "^1.0.4",
    "gulp": "^3.9.1",
    "json-loader": "^0.5.4",
    "karma": "^1.4.0",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-json-preprocessor": "^0.3.3",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^1.3.3",
    "tslint": "^4.3.1",
    "typescript": "^2.1.4",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.14.0",
    "webpack-fail-plugin": "^1.0.5"
  }
}
