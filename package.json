{
  "name": "font-awesome-openui5",
  "version": "2.0.2",
  "description": "Font Awesome icons for OpenUI5",
  "main": "./main/font-awesome-openui5.js",
  "typings": "./index.d.ts",
  "scripts": {
    "build:unmin": "webpack --bail",
    "build:min": "cross-env NODE_ENV=production webpack --bail --output-filename font-awesome-openui5.min.js",
    "build": "npm run clean && npm run build:unmin && npm run build:min && npm run tsc",
    "clean": "rimraf ./dist ./main",
    "tsc": "tsc -p . --sourceMap false",
    "pretest": "gulp count:test",
    "test:sync": "karma start ./karma/sync.conf.js",
    "test:async": "karma start ./karma/async.conf.js",
    "test:autoload": "karma start ./karma/autoload.conf.js",
    "test": "npm run test:sync && npm run test:async && npm run test:autoload",
    "test:ffdev": "npm run test:sync -- --browsers FirefoxDeveloper && npm run test:async -- --browsers FirefoxDeveloper && npm run test:autoload -- --browsers FirefoxDeveloper"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/font-awesome-openui5.git"
  },
  "keywords": [
    "font-awesome",
    "font awesome",
    "openui5"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/font-awesome-openui5/issues"
  },
  "homepage": "https://gluons.github.io/font-awesome-openui5",
  "dependencies": {
    "font-awesome-icon-chars": "^1.2.2",
    "lodash.defer": "^4.1.0",
    "nvl": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "6",
    "@types/openui5": "^1.40.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "chai": "*",
    "cross-env": "^3.1.4",
    "eslint": "^3.14.0",
    "eslint-config-gluons": "^1.0.4",
    "get-env": "^0.5.10",
    "gulp": "^3.9.1",
    "karma": "^1.4.0",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-json-preprocessor": "^0.3.3",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "mocha": "*",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^2.0.0",
    "tslint": "^4.3.1",
    "typescript": "^2.1.6",
    "uglify-js": "^2.7.5",
    "uglifyjs-webpack-plugin": "^0.1.3",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^2.2.1"
  }
}
